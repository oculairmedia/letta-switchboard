# Letta Switchboard Configuration

# Encryption key for schedule data at rest
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
LETTA_SWITCHBOARD_ENCRYPTION_KEY=

# Base URL for the service (used in API responses)
LETTA_SWITCHBOARD_BASE_URL=http://localhost:8085

# Letta API base URL (for self-hosted Letta instances)
# Default: https://api.letta.com
LETTA_BASE_URL=https://letta.oculair.ca

# Dev mode - disable encryption for debugging (true/false)
LETTA_SWITCHBOARD_DEV_MODE=false
